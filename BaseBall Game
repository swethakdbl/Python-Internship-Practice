class Solution:
    def calPoints(self, operations:List[str]) -> int:
        lst=[]
        for op in operations:
            if op=="C":
                lst.pop()
            elif op=="D":
                lst.append(2*1[-1])
            elif op=="+":
                lst.append(1[-1]+1[-2])
            else:
                lst.append(int(op))
                print(lst)
        return sum(lst)
